{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/sign-up",
      "dest": "api/index.js"
    },
    {
      "src": "/send",
      "dest": "api/index.js"
    },
    {
      "src": "/subscribe",
      "dest": "api/index.js"
    }
  ]
}